{
    "name": "bestit/laravel-flagception",
    "description": "Laravel driver for the bestit/flagception-sdk",
    "require": {
        "flagception/flagception": "^1.1",
        "illuminate/support": "5.5.*|5.6.*",
        "illuminate/routing": "5.5.*|5.6.*",
        "illuminate/view": "5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "andre.varelmann",
            "email": "andre.varelmann@bestit-online.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "BestIt\\LaravelFlagception\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BestIt\\LaravelFlagception\\FlagceptionServiceProvider"
            ],
            "aliases": {
                "Flagception": "BestIt\\LaravelFlagception\\Flagception"
            }
        }
    }
}
